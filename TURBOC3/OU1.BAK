#include<iostream.h>
void F(int & A, int & B, int C)
{  A = B + C ;
   B  = A+2 ;
   cout<<"A="<<A<<"B="<<B<<"C="<<C;
}
void main(){
     int x = 10,y=5;
     F(x,y,y);
     cout<<"x = "<<x<<”y=”<<y;
}

